<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>LIST</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <header>
      <a class="header-logo">LIST</a>
    </header>
    <div class="container">
      <div class="container-header">
        <h1>Shopping List</h1>
      </div>
      <div class="index-table-wrapper">
        <div class="table-head">
          <span class="id-column">ID</span>
          <span>Item</span>
        </div>
        <ul class="table-body">
          <li>
            <span class="id-column">1</span>
            <span class="name-column">potatoes</span>
          </li>
          <li>
            <span class="id-column">2</span>
            <span class="name-column">carrots</span>
          </li>
          <li>
            <span class="id-column">3</span>
            <span class="name-column">onions</span>
          </li>
        </ul>
      </div>
    </div>
  </body>
</html>